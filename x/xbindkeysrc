"pamixer --increase 5 && notify-send -u low 'Volume Increased' "Now set to $(pamixer --get-volume)" -t 5"
    XF86AudioRaiseVolume

"pamixer --decrease 5 && notify-send -u low 'Volume Decreased' "Now set to $(pamixer --get-volume)" -t 5"
    XF86AudioLowerVolume

"pamixer --mute && notify-send -u low 'Audio Muted' "Now set to $(pamixer --get-volume)" -t 5"
    XF86AudioMute

"light -A 2%"
    XF86KbdBrightnessUp

"light -U 2%"
    XF86KbdBrightnessDown
