alias annexa="git annex add"
alias annexd="git annex drop"
alias annexm="git annex metadata"
alias b="light -S "
alias con="connmanctl"
alias grep="grep --color=auto --exclude-dir=.cvs --exclude-dir=.git --exclude-dir=.hg --exclude-dir=.svn"
alias less="less -R"
alias ls="ls -laht --color"
alias rsync="rsync -avhP"
